         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label fx:id="done" layoutX="265.0" layoutY="288.0" prefHeight="55.0" prefWidth="70.0" text="Done!" visible="false">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Label layoutX="34.0" layoutY="356.0" prefHeight="39.0" prefWidth="190.0" text="Scroll to Zoom">
